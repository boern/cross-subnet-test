type Counter = variant {
  topic : text;
  value : nat64;
};
type CallTime = variant {
  before_call : nat64;
  after_call : nat64;
  delay : nat64;
};
type Subscriber = variant {
  topic : text;
};
service : {
  "setup_subscribe" : (publisher_id : principal, topic : text) -> ();
  "update_count" : (counter : Counter) -> ();
  "get_count" : () -> (nat64);
  "get_inner_call_time" : () -> (CallTime) query;
};
